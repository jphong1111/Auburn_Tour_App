<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.AuburnTour">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_auburn_icon_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_auburn_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key"/>


        <activity android:name=".Flash_Activity"
            android:theme="@style/SplashTheme"
            android:screenOrientation="portrait"
            >

                <intent-filter>
                    <action android:name="android.intent.action.MAIN" />
                    <action android:name="android.intent.action.VIEW"/>
                    <category android:name="android.intent.category.LAUNCHER" />

                </intent-filter>

        </activity>


        <activity android:name="com.example.AuburnTour.MainActivity">

        </activity>
        <activity android:name="com.example.AuburnTour.sub1_Activity"/>
        <activity android:name="com.example.AuburnTour.sub12_Activity"/>

        <activity android:name="com.example.AuburnTour.sub2_Activity"/>

        <activity android:name="com.example.AuburnTour.sub3_Activity"/>
        <activity android:name="com.example.AuburnTour.sub4_Activity"/>
        <activity android:name="com.example.AuburnTour.sub41_Activity"/>


        </application>


</manifest>